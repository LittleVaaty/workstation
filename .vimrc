
" ==============================
"        Basic Settings 
" ==============================
set number
set relativenumber
set nocompatible
set mouse=a
set cursorline

syntax enable
filetype plugin on

" ==============================
"         keymaps
" ==============================
set timeoutlen=500
inoremap kj <esc>
vnoremap kj <esc>
cnoremap kj <C-C>
